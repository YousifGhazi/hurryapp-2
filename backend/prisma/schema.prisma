datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Sensors {
  id   Int   @id
  lat  Float
  long Float
}

model ReadingSensors {
  id        Int      @id @default(autoincrement())
  sensor_id Int
  temp      Float?
  hum       Float?
  co2       Float?
  pm25      Float?
  timestamp DateTime @default(now())
}
